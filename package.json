{
  "name": "native-router",
  "version": "0.0.1",
  "scripts": {
    "test": "jest",
    "build": "tsc"
  },
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "dependencies": {},
  "files": [
    "dist/",
    "lib/"
  ],
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@types/jest": "^23.3.13",
    "@types/react": "^16.7.21",
    "@types/react-native": "^0.57.32",
    "@types/react-test-renderer": "^16.0.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "24.0.0",
    "jest": "24.0.0",
    "metro-react-native-babel-preset": "0.51.1",
    "react": "^16.7.0",
    "react-native": "^0.58.3",
    "react-native-screens": "^1.0.0-alpha.22",
    "react-test-renderer": "16.6.3",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.4"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-screens": "^1.0.0-alpha.22"
  },
  "jest": {
    "verbose": true,
    "preset": "react-native",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "\\.snap$",
      "node_modules/",
      "example"
    ],
    "transformIgnorePatterns": [
      "example"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/example"
    ]
  }
}
